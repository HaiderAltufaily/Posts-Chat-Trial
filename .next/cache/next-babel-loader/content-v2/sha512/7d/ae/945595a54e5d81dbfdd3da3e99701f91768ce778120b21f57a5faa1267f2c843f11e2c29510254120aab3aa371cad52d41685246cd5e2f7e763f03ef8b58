{"ast":null,"code":"import { setDoc } from \"@firebase/firestore\";\nimport axios from \"axios\";\nimport { useEffect, useState } from \"react\";\n\nconst useFetch = ({\n  url,\n  method = \"get\",\n  body = null\n}, applyData) => {\n  const {\n    0: isLoading,\n    1: setIsLoading\n  } = useState(true);\n  const {\n    0: error,\n    1: setError\n  } = useState(null);\n  const {\n    0: data,\n    1: setData\n  } = useState([]); //   console.log(method, url, body);\n\n  async function getData() {\n    await axios[method](url, body).then(data => {\n      setData(data.data);\n      applyData(data);\n      setIsLoading(false);\n    }).catch(err => {\n      setError(err.message);\n      setIsLoading(false);\n    });\n  }\n\n  useEffect(() => {\n    getData();\n  }, []);\n  return {\n    isLoading,\n    error,\n    data,\n    getData\n  };\n};\n\nexport default useFetch;","map":{"version":3,"sources":["D:/Udemy/nextjs/06-onwards-to-a-bigger-project-starting-project/hooks/useFetch.js"],"names":["setDoc","axios","useEffect","useState","useFetch","url","method","body","applyData","isLoading","setIsLoading","error","setError","data","setData","getData","then","catch","err","message"],"mappings":"AAAA,SAASA,MAAT,QAAuB,qBAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,OAApC;;AAEA,MAAMC,QAAQ,GAAG,CAAC;AAAEC,EAAAA,GAAF;AAAOC,EAAAA,MAAM,GAAG,KAAhB;AAAuBC,EAAAA,IAAI,GAAG;AAA9B,CAAD,EAAuCC,SAAvC,KAAqD;AACpE,QAAM;AAAA,OAACC,SAAD;AAAA,OAAYC;AAAZ,MAA4BP,QAAQ,CAAC,IAAD,CAA1C;AACA,QAAM;AAAA,OAACQ,KAAD;AAAA,OAAQC;AAAR,MAAoBT,QAAQ,CAAC,IAAD,CAAlC;AACA,QAAM;AAAA,OAACU,IAAD;AAAA,OAAOC;AAAP,MAAkBX,QAAQ,CAAC,EAAD,CAAhC,CAHoE,CAIpE;;AACA,iBAAeY,OAAf,GAAyB;AACvB,UAAMd,KAAK,CAACK,MAAD,CAAL,CAAcD,GAAd,EAAmBE,IAAnB,EACHS,IADG,CACGH,IAAD,IAAU;AACdC,MAAAA,OAAO,CAACD,IAAI,CAACA,IAAN,CAAP;AACAL,MAAAA,SAAS,CAACK,IAAD,CAAT;AACAH,MAAAA,YAAY,CAAC,KAAD,CAAZ;AACD,KALG,EAMHO,KANG,CAMIC,GAAD,IAAS;AACdN,MAAAA,QAAQ,CAACM,GAAG,CAACC,OAAL,CAAR;AACAT,MAAAA,YAAY,CAAC,KAAD,CAAZ;AACD,KATG,CAAN;A